{
    "configurations": [
        {
            "name": "GCC AmigaOS 3",
            "includePath": [
                "./**",
                "/opt/amiga/m68k-amigaos/include",
                "/opt/amiga/m68k-amigaos/ndk-include",
                "/opt/amiga/m68k-amigaos/libnix/include",
                "/opt/amiga/m68k-amigaos/sys-include",
                "/opt/amiga/m68k-amigaos/clib2/include"
            ],
            "defines": ["__AMIGAOS3__", "PROGRAM_NAME=\"AmigaGPT\"","__stdargs= ", "PROGDIR=\"PROGDIR:\""],
            "macFrameworkPath": [
                "/Applications/Xcode.app/Contents/Developer/Platforms/MacOSX.platform/Developer/SDKs/MacOSX.sdk/System/Library/Frameworks"
            ],
            "cStandard": "c17",
            "cppStandard": "c++20",
            "intelliSenseMode": "macos-gcc-arm64",
            "mergeConfigurations": false,
            "compilerPath": "/opt/amiga/bin/m68k-amigaos-gcc",
            "browse": {
                "path": [
                    "${workspaceFolder}"
                ],
                "limitSymbolsToIncludedHeaders": true
            }
        },
        {
            "name": "GCC AmigaOS 4",
            "includePath": [
                "./**",
                "/opt/amiga/ppc-amigaos/SDK/newlib/include",
                "/opt/amiga/ppc-amigaos/SDK/include/include_h",
                "/opt/amiga/ppc-amigaos/SDK/include/netinclude",
                "/opt/amiga/ppc-amigaos/SDK/local/common/include",
                "/opt/amiga/ppc-amigaos/SDK/local/newlib/include"
            ],
            "defines": ["__AMIGAOS4__", "__USE_INLINE__", "PROGRAM_NAME=\"AmigaGPT\"", "PROGDIR=\"PROGDIR:\""],
            "macFrameworkPath": [
                "/Applications/Xcode.app/Contents/Developer/Platforms/MacOSX.platform/Developer/SDKs/MacOSX.sdk/System/Library/Frameworks"
            ],
            "cStandard": "c17",
            "cppStandard": "c++20",
            "intelliSenseMode": "macos-gcc-arm64",
            "mergeConfigurations": false,
            "compilerPath": "/opt/amiga/bin/m68k-amigaos-gcc",
            "browse": {
                "path": [
                    "${workspaceFolder}"
                ],
                "limitSymbolsToIncludedHeaders": true
            }
        },
        {
            "name": "Mac",
            "includePath": [
                "${workspaceFolder}/**",
                "/opt/homebrew/include/"
            ],
            "defines": [],
            "macFrameworkPath": [
                "/Library/Developer/CommandLineTools/SDKs/MacOSX.sdk/System/Library/Frameworks"
            ],
            "compilerPath": "/opt/homebrew/bin/gcc-12",
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "macos-clang-arm64"
        }
    ],
    "version": 4
}